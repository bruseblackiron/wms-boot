<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.wms.mapper.TbWareHouseMapper">
    
    <resultMap type="TbWareHouse" id="TbWareHouseResult">
        <result property="id"    column="id"    />
        <result property="wareHouseName"    column="ware_house_name"    />
        <result property="isPublish"    column="is_publish"    />
        <result property="isDelete"    column="is_delete"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectTbWareHouseVo">
        select id, ware_house_name, is_publish, is_delete, create_by, create_time, update_by, update_time, remark from tb_ware_house
    </sql>

    <select id="selectTbWareHouseList" parameterType="TbWareHouse" resultMap="TbWareHouseResult">
        <include refid="selectTbWareHouseVo"/>
        <where>  
            <if test="wareHouseName != null  and wareHouseName != ''"> and ware_house_name like concat('%', #{wareHouseName}, '%')</if>
            <if test="isPublish != null "> and is_publish = #{isPublish}</if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''"> and create_time between #{params.beginCreateTime} and #{params.endCreateTime}</if>
        </where>
    </select>
    
    <select id="selectTbWareHouseById" parameterType="Long" resultMap="TbWareHouseResult">
        <include refid="selectTbWareHouseVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTbWareHouse" parameterType="TbWareHouse" useGeneratedKeys="true" keyProperty="id">
        insert into tb_ware_house
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="wareHouseName != null and wareHouseName != ''">ware_house_name,</if>
            <if test="isPublish != null">is_publish,</if>
            <if test="isDelete != null">is_delete,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="wareHouseName != null and wareHouseName != ''">#{wareHouseName},</if>
            <if test="isPublish != null">#{isPublish},</if>
            <if test="isDelete != null">#{isDelete},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateTbWareHouse" parameterType="TbWareHouse">
        update tb_ware_house
        <trim prefix="SET" suffixOverrides=",">
            <if test="wareHouseName != null and wareHouseName != ''">ware_house_name = #{wareHouseName},</if>
            <if test="isPublish != null">is_publish = #{isPublish},</if>
            <if test="isDelete != null">is_delete = #{isDelete},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTbWareHouseById" parameterType="Long">
        delete from tb_ware_house where id = #{id}
    </delete>

    <delete id="deleteTbWareHouseByIds" parameterType="String">
        delete from tb_ware_house where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>